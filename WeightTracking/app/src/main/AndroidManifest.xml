<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.weighttracking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WeightTracking">
        <activity android:name=".MainActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" android:exported="true" />
        <activity android:name=".LandingPageActivity" android:exported="true" />
        <activity android:name=".DataDisplayActivity" android:exported="true" />
        <activity android:name=".SMSActivity" android:exported="true" />
        <activity android:name=".SMSPermissionActivity" android:exported="true" />
    </application>
</manifest>
